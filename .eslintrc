{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true
  },
  "rules": {
    "quotes": [ 1, "single", "avoid-escape"],
    "eol-last":0,
    "no-undef":0,
    "global-strict":0,
    "no-extra-semi":1,
    "no-underscore-dangle":0,
    "semi":1,
    "semi-spacing":0,
    "no-unused-vars":1
  }
}
